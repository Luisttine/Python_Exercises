def is_valid_IP(strng):

    lista = strng.split(".")

    check = 0
    for i in lista:
        ints = []
        ints.append(int(i))
        check = check + 1
            
    if check == 4:
        for k in lista:
            if ints[k] > 255 and ints[k] < 1:
                return False
            else:
                return strng
            

        return lista

print(is_valid_IP("1.2.3.4"))
